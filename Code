int w = Integer.parseInt(jTextField1.getText());
int x = Integer.parseInt(jTextField1.getText());
int y = Integer.parseInt(jTextField2.getText());
int z = Integer.parseInt(jTextField3.getText());



int ave = w + x + y + z ;
double perc = ave / 4;


jTextField5.setText(String.valueOf(perc));
       
JOptionPane.showMessageDialog(this, "COMPUTE");
